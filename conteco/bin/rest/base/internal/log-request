#!/usr/bin/env bash
reqFile="$1"

source="rest_api"
level="INITIATED"
request="$(cat $reqFile)"
request=$( echo "$request" | jq -sR . )
timestamp=$( to-timestamp )
printf '{ "origin": "%s", "@timestamp": "%s", "source": "%s", "level": "%s", "request":%s, "response":"" }\n' "$CONTECO_IMAGE" "$timestamp" "$source" "$level" "$request"

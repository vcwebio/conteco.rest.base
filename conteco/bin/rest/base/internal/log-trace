#!/usr/bin/env bash
dumpFile="$1"

source="rest_trace"
level="TRACE"
trace="$(cat $dumpFile)"
trace=$( echo "$trace" | jq -sR . )
timestamp=$( to-timestamp )
printf '{ "origin": "%s", "@timestamp": "%s", "source": "%s", "level": "%s", "trace":%s}\n' "$CONTECO_IMAGE" "$timestamp" "$source" "$level" "$trace"
